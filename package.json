{
   "name": "koalerplate",
   "homepage": "https://github.com/dbalas/koalerplate.git",
   "description": "A simple Koa 2 boilerplate for API's using ES6.",
   "bugs": {
      "url": "https://github.com/dbalas/koalerplate/issues/new"
   },
   "keywords": [
      "koa",
      "koa2",
      "boilerplate",
      "koalerplate",
      "api",
      "es6"
   ],
   "repository": "git@github.com:dbalas/koalerplate.git",
   "author": "Daniel Balastegui",
   "license": "MIT",
   "version": "1.4.0",
   "main": "index.js",
   "scripts": {
      "dev": "nodemon index",
      "start": "ts-node src/index.ts",
      "go": "tsc && nodemon build/index",
      "start-db": "pg_ctl -D /usr/local/var/postgres start",
      "stop-db": "pg_ctl -D /usr/local/var/postgres stop",
      "test": "tsc && jest build/test"
   },
   "dependencies": {
      "@koa/cors": "^3.0.0",
      "class-validator": "^0.9.1",
      "cross-env": "^5.2.0",
      "dotenv": "^7.0.0",
      "koa": "^2.6.2",
      "koa-bodyparser": "^4.2.1",
      "koa-helmet": "^4.0.0",
      "koa-logger": "^3.2.0",
      "koa-respond": "^2.1.0",
      "koa-router": "^7.4.0",
      "koa-static": "^5.0.0",
      "mongoose": "^5.5.7",
      "mysql": "^2.14.1",
      "pg": "^7.11.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.17",
      "uuid": "^3.3.2"
   },
   "devDependencies": {
      "@babel/preset-env": "^7.4.4",
      "@types/jest": "^24.0.13",
      "@types/koa": "^2.0.48",
      "@types/koa-router": "^7.0.40",
      "@types/node": "^8.0.29",
      "jest": "^24.8.0",
      "nodemon": "^1.18.9",
      "ts-jest": "^24.0.2",
      "typescript": "3.3.3333",
      "ts-node": "3.3.0"
   }
}
